--langdef=rapid
--langmap=rapid:.mod
--regex-rapid=/\<[Pp][Rr][Oo][Cc]\>[ \t]*([a-zA-Z0-9_]+)/\1/p,procedure/
--regex-rapid=/\<[Ff][Uu][Nn][Cc]\>[ \t]*([a-zA-Z]+)[ \t]*([a-zA-Z0-9_]+)/\2/f,function/
--regex-rapid=/\<[Tt][Rr][Aa][Pp]\>[ \t]*([a-zA-Z0-9_]+)/\1/t,trap/

--langdef=krl
--langmap=krl:.src
--regex-krl=/\<[Dd][Ee][Ff]\>[ \t]*([a-zA-Z0-9_]+)/\1/p,procedure/
--regex-krl=/\<[Cc][Aa][Ss][Ee]\>[ \t]*([a-zA-Z0-9_]+)/\1/c,case/
--regex-krl=/\<[Dd][Ee][Ff][Ff][Cc][Tt]\>[ \t]*([a-zA-Z]+)[ \t]*([a-zA-Z0-9_]+)/\2/f,funktion/

